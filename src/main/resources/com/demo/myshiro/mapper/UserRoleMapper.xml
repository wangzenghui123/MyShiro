<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.myshiro.dao.UserRoleDao">


    <select id="queryUserRoleByUserId" resultType="com.demo.myshiro.entity.UserRole" parameterType="java.lang.String">
        select id,userId,roleId from t_user_role where userId = #{userId}
    </select>

</mapper>